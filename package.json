{
  "name": "sorting-visualizer",
  "version": "1.0.0",
  "description": "Visualizer of various sorting algorithms",
  "default": "index.js",
  "scripts": {
    "compile:sass": "sass ./src/sass/sorting-visualizer.scss ./css/sorting-visualizer.css --watch",
    "start": "parcel index.html",
    "build": "parcel build index.html --dist-dir ./dist"
  },
  "keywords": [
    "Sorting",
    "bubble",
    "selection",
    "insertion",
    "merge",
    "quick",
    "radix"
  ],
  "author": "Haris Semic",
  "license": "ISC",
  "devDependencies": {
    "parcel": "^2.0.0-beta.1",
    "sass": "^1.32.8"
  },
  "dependencies": {
    "core-js": "^3.9.1",
    "regenerator-runtime": "^0.13.7"
  }
}
